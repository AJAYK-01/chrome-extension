{
    "name": "Vibinex",
    "version": "0.0.1",
    "manifest_version": 2,
    "description":"for making changes in github website",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "vibinex-logo.png"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "128": "vibinex-logo.png"
    },
    "content_scripts":[{
        "matches":["https://github.com/*pull*","https://github.com/*pulls*"],
        "css":["main.css"],
        "js":["main.js"]
    }]
}